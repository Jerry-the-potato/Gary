{
  "name": "src",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:bg": "start /b vite",
    "dev:detached": "powershell -Command \"Start-Process -FilePath 'npm' -ArgumentList 'run dev' -WindowStyle Hidden\"",
    "dev:new-window": "start cmd /k npm run dev",
    "stop:dev": "taskkill /f /im node.exe",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "pinia": "^3.0.3",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@types/node": "^24.2.1",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "@webgpu/types": "^0.1.64",
    "canvas": "^3.1.2",
    "jsdom": "^26.1.0",
    "typescript": "~5.8.3",
    "vite": "^5.4.10",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.5"
  }
}
